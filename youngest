// nested if

#include<stdio.h>
int main(){
    
    int x,y,z;
    printf("Age of ram :");
    scanf("%d",&x);
    printf("Age of shyam :");
    scanf("%d",&y);
    printf("Age of ajay :");
    scanf("%d",&z);
    
    if(x<y){
        if(x<z)
           printf("Ram is the youngest");
        else
        printf("Ajay is the youngest");
    }
    else{
        if(y<z)
        printf("Shyam is the youngest");
        else
        printf("Ajay is the youngest");
    }
    
    return 0;
}
