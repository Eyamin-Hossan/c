#include <stdio.h>

int main()
{
    int x,y,z;
    printf("Enter the 1st number : ");
    scanf("%d",&x);
    printf("Enter the 2nd number : ");
    scanf("%d",&y);
    printf("Enter the 3rd number : ");
    scanf("%d",&z);
    
    if(x>y && x>z){
        printf("%d is the greatest",x);
    }
    if(y>x && y>z){
        printf("%d is the greatest",y);
    }
    else{
        printf("%d is the greatest",z);
    }
    return 0;
}
