// ternary operator
// exp1 ? exp2 (yes) : exp3 (no) ;

#include <stdio.h>

int main()
{
    int n;
    printf("Wnter a number :");
    scanf("%d",&n);
    // if(n%2==0)
    // printf("Even number");
    // else
    // printf("Odd number");
    
    n%2==0 ? printf("Even number") : printf("Odd number") ;
    

    return 0;
}
